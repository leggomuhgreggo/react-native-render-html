{
  "exclude": "node_modules/**",
  "presets": [
    ["@babel/env", {
      corejs: 3,
      modules: false,
      useBuiltIns: 'usage',
      targets: {
        ie: '11',
      },
    }],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["@babel/plugin-transform-runtime", {
      "regenerator": true
    }]
  ]
}
